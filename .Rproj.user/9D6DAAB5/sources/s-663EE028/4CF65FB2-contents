% Please edit documentation in R/kernel3D.R
\name{Kernel3D}
\alias{Kernel3D}
\title{3-dim Kernel function for longitudinal data to get Pr(y1(t1),y2(t2)|x(t1))}
\usage{
Kernel3D(IDls = ID, Y, Time, X, T1, T2, X0, Bndwdth1, Bndwdth2, Bndwdth3)
}
\arguments{
\item{IDls}{the vector of subject ID in a longitudinal sample}

\item{Y, X, Time}{numeric vectors of outcome, covariate and time of the the same length}

\item{T1, T2}{twp given time points}

\item{X0}{a given covariate value}

\item{Bndwdth1, Bndwdth2, Bndwdth3}{three bandwidths around two time and one covariate value}
}
\value{
3-dim Kernel function results
}
\description{
3-dim Kernel function for longitudinal data to get Pr(y1(t1),y2(t2)|x(t1))
}
\references{
{ Wu, C.O. and Tian, X.  Nonparametric Models for Longitudinal Data: With Implementation in R. Chapman & Hall/CRC.
            2018.}
}
